@using WebApplication2.Models;
@{
    Layout = null;
    List<Telephon_DB> book = ViewBag.TelephoneBook;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
       
        @foreach (Telephon_DB row in book)
        {@row.Id
        @row.Surname
        @row.TelephoneNumber
        <br />
    }

        <h2> <a href="~/Dict/Add"> Add </a></h2>
        <h2> <a href="~/Dict/Update"> Update </a></h2>
        <h2> <a href="~/Dict/Delete"> Delete </a></h2>

    </div>
</body>
</html>
